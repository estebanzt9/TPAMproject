<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            motion:layout_editor_absoluteY="1dp"
            android:layout_height="645dp"
            android:layout_width="409dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout2" />
        <Constraint
            motion:layout_editor_absoluteY="26dp"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView"
            motion:layout_constraintEnd_toEndOf="parent" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="708dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="407dp"
            android:id="@+id/layout2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteX="4dp"
            motion:layout_editor_absoluteY="23dp" />
        <Constraint
            android:layout_height="729dp"
            motion:layout_editor_absoluteX="4dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/cardView" />
        <Constraint
            android:layout_height="721dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_width="830dp"
            android:id="@+id/imageView2" />
        <Constraint
            android:layout_width="407dp"
            android:layout_height="708dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/layout2"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_editor_absoluteX="4dp"
            motion:layout_editor_absoluteY="23dp" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="409dp"
            android:layout_height="81dp"
            android:layout_marginStart="1dp"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteY="628dp"
            android:id="@+id/saludo" />
        <Constraint
            motion:layout_editor_absoluteY="133dp"
            motion:layout_editor_absoluteX="229dp"
            android:id="@+id/button2" />
        <Constraint
            motion:layout_editor_absoluteY="474dp"
            motion:layout_editor_absoluteX="-4dp"
            android:id="@+id/saludo2" />
        <Constraint
            motion:layout_editor_absoluteY="127dp"
            motion:layout_editor_absoluteX="-3dp"
            android:id="@+id/linearLayout2" />
        <Constraint
            motion:layout_editor_absoluteY="476dp"
            android:id="@+id/button7"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_editor_absoluteX="263dp" />
        <Constraint
            motion:layout_editor_absoluteY="0dp"
            motion:layout_editor_absoluteX="-3dp"
            android:id="@+id/constraintLayout2" />
        <Constraint
            motion:layout_editor_absoluteY="431dp"
            motion:layout_editor_absoluteX="-24dp"
            android:id="@+id/mapView" />
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/Enter id"
        motion:deriveConstraintsFrom="@+id/start" />
</MotionScene>